import macierze

x = [
    [0, 2, 4], 
    [1, 3, 5], 
    [7, 8, 9]]

y = [[1, 2, 3], 
     [4, 5, 6], 
     [7, 10, 9]]


wynik_mnozenia = macierze.mnozenie_macierzy(x, y)
print(f"Wynik mno≈ºenia macierzy: {wynik_mnozenia}")

wynik_dodawania = macierze.dodawanie_macierzy(x, y)
print(f"Wynik dodawania macierzy: {wynik_dodawania}")

wynik_odejmowania = macierze.odejmowanie_macierzy(x, y)
print(f"Wynik odejmowania macierzy: {wynik_odejmowania}")

transpozycja_1 = macierze.transpozycja_macierzy(x)
print(f"Transpozycja macierzy 1: {transpozycja_1}")

transpozycja_2 = macierze.transpozycja_macierzy(y)
print(f"Transpozycja macierzy 2: {transpozycja_2}")

